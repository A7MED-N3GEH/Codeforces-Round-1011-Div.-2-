#include <iostream>
#include <algorithm>
using namespace std;

typedef long long ll;

void solve() {
    ll x, y;
    cin >> x >> y;
    if (x == y) {
        cout << -1 << endl;
        return;
    }
    cout << (1LL << 30) - max(x, y) << endl;
}

int main() {
    int t;
    cin >> t;
    while (t--) {
        solve();
    }
    return 0;
}
